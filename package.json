{
  "name": "fuck-smithery",
  "version": "1.0.0",
  "description": "MCP-Init: A toolkit for building Model Context Protocol servers with best-in-class DX",
  "main": "index.js",
  "workspaces": [
    "cli-core",
    "scan",
    "wizard", 
    "dev-loop",
    "deploy",
    "analytics",
    "playground"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces",
    "dev": "cd cli-core && npm run dev",
    "clean": "npm run clean --workspaces && rimraf node_modules",
    "install:all": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazumah1/fuck-smithery.git"
