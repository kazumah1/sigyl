{
  "name": "@mcp-platform/registry-api",
  "version": "0.1.0",
  "description": "MCP Registry API - Express server for MCP package registry",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist",
    "seed": "ts-node src/scripts/seedData.ts",
    "migrate:api-keys": "ts-node src/scripts/runMigrations.ts",
    "manage-keys": "ts-node src/scripts/manageApiKeys.ts",
    "test:api-keys": "ts-node src/scripts/testApiKeys.ts",
    "test:secrets": "ts-node src/scripts/testSecrets.ts",
    "test:contact": "ts-node src/scripts/testContactEmail.ts",
    "debug:secrets": "ts-node src/scripts/debugSecrets.ts",
    "validate-env": "ts-node src/scripts/validate-env.ts",
    "prestart": "npm run validate-env",
    "predeploy": "npm run validate-env"
  },
  "dependencies": {
    "@google-cloud/cloudbuild": "^5.1.0",
    "@google-cloud/run": "^2.1.0",
    "@octokit/rest": "^20.1.2",
    "@sigil/container-builder": "file:../container-builder",
    "@supabase/supabase-js": "^2.38.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-fetch": "^2.6.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "google-auth-library": "^10.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "octokit": "^5.0.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "mcp",
    "registry",
    "api",
    "express",
    "supabase"
  ],
  "author": "Sigil Team",
  "license": "MIT"
}
